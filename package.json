{
  "name": "@e22m4u/js-path-trie",
  "version": "0.0.12",
  "description": "Маршрутизатор для Node.js на основе префиксного дерева",
  "author": "Mikhail Evstropov <e22m4u@yandex.ru>",
  "license": "MIT",
  "keywords": [
    "path",
    "trie",
    "router"
  ],
  "homepage": "https://github.com/e22m4u/js-path-trie",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/e22m4u/js-path-trie.git"
  },
  "type": "module",
  "types": "./src/index.d.ts",
  "module": "./src/index.js",
  "main": "./dist/cjs/index.cjs",
  "exports": {
    "types": "./src/index.d.ts",
    "import": "./src/index.js",
    "require": "./dist/cjs/index.cjs"
  },
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "lint": "tsc && eslint ./src",
    "lint:fix": "tsc && eslint ./src --fix",
    "format": "prettier --write \"./src/**/*.js\"",
    "test": "npm run lint && c8 --reporter=text-summary mocha",
    "test:coverage": "npm run lint && c8 --reporter=text mocha",
    "build:cjs": "rimraf ./dist/cjs && node --no-warnings=ExperimentalWarning build-cjs.js",
    "prepare": "husky"
  },
  "dependencies": {
    "@e22m4u/js-debug": "~0.3.1",
    "@e22m4u/js-format": "~0.2.0",
    "path-to-regexp": "~8.3.0"
  },
  "devDependencies": {
    "@commitlint/cli": "~20.1.0",
    "@commitlint/config-conventional": "~20.0.0",
    "@eslint/js": "~9.36.0",
    "c8": "~10.1.3",
    "chai": "~6.2.0",
    "esbuild": "~0.25.10",
    "eslint": "~9.36.0",
    "eslint-config-prettier": "~10.1.8",
    "eslint-plugin-chai-expect": "~3.1.0",
    "eslint-plugin-jsdoc": "~60.6.0",
    "eslint-plugin-mocha": "~11.1.0",
    "globals": "~16.4.0",
    "husky": "~9.1.7",
    "mocha": "~11.7.3",
    "prettier": "~3.6.2",
    "rimraf": "~6.0.1",
    "typescript": "~5.9.3"
  }
}
